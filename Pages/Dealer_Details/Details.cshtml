@page
@model PropertyManagement.Pages.Dealer_Details.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <!--Labels shows in dealer detais table-->
    <h4>Dealer_Detail</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Dealer Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Dealer_Detail.Dealer_Name)
        </dd>
        <dt class="col-sm-2">
            Dealer Address
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Dealer_Detail.Dealer_Address)
        </dd>
        <dt class="col-sm-2">
           Phone No.
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Dealer_Detail.Mobile)
        </dd>
        <dt class="col-sm-2">
            Email Address
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Dealer_Detail.Email)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Dealer_Detail.Id">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
